{
    "name": "starter-vite-react-redux_toolkit-typescript_scss",
    "scripts": {
        "start": "vite",
        "test": "vitest",
        "build": "tsc && vite build",
        "build-preview": "vite preview",
        "update": "pnpm update || yarn upgrade || npm update",
        "prettier": "prettier --write src/**/*",
        "lint": "eslint src/**/*.{ts,tsx}  --report-unused-disable-directives && echo '\u001b[32mESLint n'a trouvé aucune erreur\u001b[0m'",
        "lint & fix": "eslint src/**/*.{ts,tsx} --fix  --report-unused-disable-directives && echo '\u001b[32mESLint n'a trouvé aucune erreur\u001b[0m'",
        "clean": "rd /s /q build",
        "clean:all": "rd /s /q node_modules build & rd /s /q build"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@reduxjs/toolkit": "^1.9.7",
        "@stylistic/eslint-plugin": "^1.4.1",
        "@testing-library/dom": "^9.3.3",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.1",
        "@types/node": "^20.10.0",
        "@types/react": "^18.2.39",
        "@types/react-dom": "^18.2.17",
        "@types/react-redux": "^7.1.31",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "@vitejs/plugin-react": "^4.2.0",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "jsdom": "^22.1.0",
        "prettier": "3.1.0",
        "react-redux": "^8.1.3",
        "sass": "^1.69.5",
        "typescript": "^5.3.2",
        "vite": "^4.5.0",
        "vitest": "^0.34.6"
    }
}
